{
    "name": "@box-extractor/core",
    "version": "0.1.7",
    "repository": {
        "type": "git",
        "url": "https://github.com/astahmer/vite-box-extractor.git"
    },
    "author": "Alexandre Stahmer <alexandre.stahmer@gmail.com>",
    "main": "dist/box-extractor-core.cjs.js",
    "module": "dist/box-extractor-core.esm.js",
    "types": "dist/box-extractor-core.cjs.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "module": "./dist/box-extractor-core.esm.js",
            "default": "./dist/box-extractor-core.cjs.js"
        }
    },
    "scripts": {
        "test": "vitest",
        "test:ci": "vitest run",
        "lint:ts": "tsc --noEmit",
        "lint": "../../node_modules/.bin/eslint './src/**/*.ts' --cache --format=pretty",
        "lint:fix": "../../node_modules/.bin/eslint './src/**/*.ts' --cache --format=pretty --fix"
    },
    "license": "ISC",
    "dependencies": {
        "@phenomnomnominal/tsquery": "^5.0.0",
        "@types/debug": "^4.1.7",
        "debug": "^4.3.4",
        "microdiff": "^1.3.1",
        "pastable": "^2.0.10",
        "ts-evaluator": "^1.0.7",
        "ts-morph": "^16.0.0",
        "ts-pattern": "^4.0.5"
    },
    "peerDependencies": {
        "typescript": ">= 4.6.3 <= 4.8.4",
        "vite": "^3"
    },
    "devDependencies": {
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@vanilla-extract/sprinkles": "^1.5.1",
        "esbuild": "^0.16.7",
        "prettier": "^2.7.1",
        "ts-toolbelt": "^9.6.0",
        "typescript": "4.8.4",
        "vite": "^3.0.4",
        "vitest": "^0.25.8"
    },
    "preconstruct": {
        "entrypoints": [
            "index.ts"
        ],
        "___experimentalFlags_WILL_CHANGE_IN_PATCH": {
            "exports": true
        }
    },
    "files": [
        "src",
        "dist",
        "../../README.md"
    ],
    "keywords": [
        "box",
        "extractor",
        "ast",
        "static",
        "prop",
        "jsx",
        "tsx",
        "typescript",
        "vite",
        "vanilla-extract",
        "esbuild",
        "sprinkles"
    ],
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    }
}
