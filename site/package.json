{
    "name": "@box-extractor/site",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "tsx ./src/server/index.ts",
        "prod": "pnpm run build && pnpm run server:prod",
        "build": "vite build",
        "preview": "vite preview",
        "start": "cross-env NODE_ENV=production tsx ./src/server/index.ts",
        "format": "prettier --write \"./{src,tests}/**/*.{ts,tsx,mdx}\"",
        "lint:ts": "tsc --noEmit",
        "lint": "../node_modules/.bin/eslint './src/**/*.ts' --cache --format=pretty",
        "lint:fix": "../node_modules/.bin/eslint './src/**/*.ts' --cache --format=pretty --fix",
        "ci": "npm run lint && npm run lint:ts && npm run test:ci"
    },
    "devDependencies": {
