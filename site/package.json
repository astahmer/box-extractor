        "pastable": "^2.1.1",
        "path-browserify": "^1.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-resizable-panels": "^0.0.33",
        "sirv": "^2.0.2",
        "ts-evaluator": "^1.0.7",
